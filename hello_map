   	var map;
     	function initMap() {
       	map = new google.maps.Map(document.getElementById('map'),{
       		center: {lat: 40.74, lng: -73.99},
       		zoom: 12
       		});
     	}
   </script>
   <script async defer
   	src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB9xN1GQtTXFL_s547AdUWSj-84zQlX4Co&v=3&callback=initMap">
   </script>
